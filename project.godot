window/size/viewport_height=1080
window/size/initial_position_type=0
window/size/initial_position=Vector2i(600, 500)
window/dpi/allow_hidpi=false
window/vsync/vsync_mode=0

[dotnet]

project/assembly_name="Unnamed-extraction-shooter-roguelike"

[editor_plugins]

enabled=PackedStringArray("res://addons/csg_converter/plugin.cfg", "res://addons/debug_menu/plugin.cfg", "res://addons/extra_snaps/plugin.cfg", "res://addons/func_godot/plugin.cfg", "res://addons/gdUnit4/plugin.cfg", "res://addons/ggs/plugin.cfg", "res://addons/godot_resource_groups/plugin.cfg", "res://addons/godot_state_charts/plugin.cfg", "res://addons/safe_resource_loader/plugin.cfg")

[global_group]

external_inventory="Inventories external to the players. Loot crates, the stash, etc"
Extract="Area for the player to extract from"
players="The human players"
viewable_entity="Anything an NPC should check if they can see"
loot_spawn_container="Container for loot to spawn into when level is populated"
loot_spawn_area="Area for loot to spawn into when the level is populated"

[godot_resource_groups]

auto_rebuild=true

[gui]

theme/custom="res://themes/minimalistic/MinimalUI.tres"
theme/custom_font="res://themes/VCR_OSD_MONO.ttf"

