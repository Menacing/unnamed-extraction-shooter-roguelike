MenuManager="*res://game_logic/menu_manager.gd"
GameplayEnums="*res://game_logic/gameplay_enums.gd"
SaveManager="*res://game_logic/save_manager.gd"
ItemMappingRepository="*res://game_logic/item_mapping_repository.gd"
